@model IEnumerable<DynamicView.Models.DynamicReportListModel>

@{
    ViewData["Title"] = "Report Prompt";
}

<h2>Enter Report ID</h2>

<form method="post" asp-action="RedirectToReport">
    <label for="reportId">Report ID:</label>
    <input type="number" id="reportId" name="reportId" class="form-control" required />
    <br />
    <button type="submit" class="btn btn-primary">Submit</button>
</form>

